{
  "require": {
    "php": ">=8.2",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "symfony/console": "v7.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.0",
    "mockery/mockery": "1.6.12"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\App\\": "tests/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit tests"
  }
}